<ul>
  {{#each savedarticles}}
    <li>
      
        <button type="button" class="btn btn-default btn-block disabled">{{this.title}}</button>
        <a href="{{this.link}}">
          <button type="button" class="btn btn-primary btn-block">{{this.link}}</button>
        </a>
      
              <form action="/saved/{{this.id}}?_method=DELETE" method="POST">
